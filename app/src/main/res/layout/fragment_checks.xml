

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="vm"
            type="com.zksg.kudoud.state.CheckFragmentViewModel" />

        <variable
            name="adapter"
            type="com.zksg.kudoud.adapters.MemeCheckListdapter" />

    </data>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/c_131722"
    >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp5"
        android:layout_marginLeft="@dimen/dp10"
        android:layout_marginRight="@dimen/dp10"
        android:visibility="gone"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"

            android:layout_centerInParent="true"
            android:text="风险级别"
            android:textColor="@color/white"
            android:textSize="@dimen/sp14" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="@string/str_f_default_n_a"
            meme_safe_level="@{vm.riskScore}"
            android:textSize="@dimen/sp12"
            android:textStyle="bold" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:visibility="gone"
            android:layout_marginTop="@dimen/dp3"
            android:src="@mipmap/ic_into1" />

    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp0.5"
        android:background="@color/c_1f2023"
        android:visibility="gone"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginRight="@dimen/dp15"
        android:layout_marginTop="@dimen/dp15"
        >
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/str_check_items"
        android:textSize="@dimen/sp16"
        android:textColor="@color/white"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:textSize="@dimen/sp12"
        android:text="0/5"
        meme_safe_5_items="@{vm.unsafeamount}"
        android:layout_alignParentRight="true"

        />

</RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_marginRight="@dimen/dp10"
        android:layout_marginTop="@dimen/dp10"
        android:orientation="horizontal"

        >

        <ImageView
            android:layout_width="@dimen/dp18"
            android:layout_height="@dimen/dp18"
            android:layout_alignParentLeft="true"
            android:layout_centerInParent="true"
            android:src="@mipmap/ic_safe"
            android:textColor="@color/primaryTextGray"
            android:textSize="@dimen/sp12"
            meme_check_token_items="@{vm.has_black_method}"
            android:textStyle="bold" />



        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:gravity="center"
            android:text="@string/str_black_function"
            android:textColor="@color/primaryTextGray"
            android:textSize="@dimen/sp12"
            android:layout_marginLeft="@dimen/dp10"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerInParent="true"
            android:layout_marginTop="@dimen/dp3"
            android:visibility="gone"
            android:src="@mipmap/ic_into1" />

    </LinearLayout>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginRight="@dimen/dp10"
        android:layout_marginTop="@dimen/dp10"
            android:orientation="horizontal"

            >

            <ImageView
                android:layout_width="@dimen/dp18"
                android:layout_height="@dimen/dp18"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:src="@mipmap/ic_safe"
                android:textColor="@color/primaryTextGray"
                android:textSize="@dimen/sp12"
                meme_check_token_items="@{vm.has_owner_removed_risk}"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/str_owner"
                android:textColor="@color/primaryTextGray"
                android:textSize="@dimen/sp12"
                android:layout_marginLeft="@dimen/dp10"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dp3"
                android:visibility="gone"
                android:src="@mipmap/ic_into1" />

        </LinearLayout>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginRight="@dimen/dp10"
            android:layout_marginTop="@dimen/dp10"
            android:orientation="horizontal"

            >

            <ImageView
                android:layout_width="@dimen/dp18"
                android:layout_height="@dimen/dp18"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:src="@mipmap/ic_safe"
                android:textColor="@color/primaryTextGray"
                android:textSize="@dimen/sp12"
                meme_check_token_items="@{vm.has_not_burned_lp}"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/str_burn_pool"
                android:textColor="@color/primaryTextGray"
                android:textSize="@dimen/sp12"
                android:layout_marginLeft="@dimen/dp10"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dp3"
                android:visibility="gone"
                android:src="@mipmap/ic_into1" />

        </LinearLayout>

    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginRight="@dimen/dp10"
            android:layout_marginTop="@dimen/dp10"
            android:orientation="horizontal"

            >

            <ImageView
                android:layout_width="@dimen/dp18"
                android:layout_height="@dimen/dp18"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:src="@mipmap/ic_safe"
                android:textColor="@color/primaryTextGray"
                android:textSize="@dimen/sp12"
                meme_check_token_items="@{vm.has_mint_method}"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/str_addition"
                android:textColor="@color/primaryTextGray"
                android:textSize="@dimen/sp12"
                android:layout_marginLeft="@dimen/dp10"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dp3"
                android:visibility="gone"
                android:src="@mipmap/ic_into1" />

        </LinearLayout>
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:layout_marginRight="@dimen/dp10"
            android:layout_marginTop="@dimen/dp10"
            android:orientation="horizontal"

            >

            <ImageView
                android:layout_width="@dimen/dp18"
                android:layout_height="@dimen/dp18"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:src="@mipmap/ic_safe"
                android:textColor="@color/primaryTextGray"
                android:textSize="@dimen/sp12"
                meme_check_token_items="@{vm.has_top10_holder_amount_over_30}"
                android:textStyle="bold" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/str_top10_30"
                android:textColor="@color/primaryTextGray"
                android:textSize="@dimen/sp12"
                android:layout_marginLeft="@dimen/dp10"
                />

            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                android:layout_marginTop="@dimen/dp3"
                android:visibility="gone"
                android:src="@mipmap/ic_into1" />

        </LinearLayout>

    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp0.5"
        android:background="@color/c_1f2023"
        android:layout_marginTop="@dimen/dp15"
        />
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginTop="@dimen/dp15"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/str_trade_gas"
            android:textColor="@color/white"
            android:textSize="@dimen/sp16"
            />



        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"

            >

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerInParent="true"
                android:orientation="vertical"

                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/str_buy_gas"
                    android:textColor="@color/primaryTextGray"
                    android:textSize="@dimen/sp12" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp5"
                    android:text="0%"
                    meme_contract_buysell_gas="@{vm.buygas}"
                    android:textColor="@color/white"
                    android:textSize="@dimen/sp12" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_alignParentRight="true"
                android:layout_margin="@dimen/dp15"
                >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/str_sell_gas"
                    android:textSize="@dimen/sp12"
                    android:textColor="@color/primaryTextGray"
                    />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0%"
                    meme_contract_buysell_gas="@{vm.sellgas}"
                    android:textColor="@color/white"
                    android:textSize="@dimen/sp12"
                    android:layout_marginTop="@dimen/dp5"
                    />

            </LinearLayout>


        </RelativeLayout>

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp0.5"
        android:background="@color/c_1f2023"
        android:layout_marginTop="@dimen/dp5"
        />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:layout_marginLeft="@dimen/dp15"
        android:layout_marginRight="@dimen/dp15"
        android:layout_marginTop="@dimen/dp15"
        >

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/str_top10_holding"
            android:textSize="@dimen/sp16"
            android:textColor="@color/white"
            />



        <com.hjq.shape.layout.ShapeRecyclerView

            linearAdapter="@{adapter}"
            cannotScrollVertically="@{false}"
            itemspace="@{1}"
            orientation="@{1}"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/dp8"
            submitList="@{vm.top10hold}"
            app:shape_radius="@dimen/dp10"
            android:padding="@dimen/dp1"

            />



    </LinearLayout>




</LinearLayout>
    </androidx.core.widget.NestedScrollView>
</layout>