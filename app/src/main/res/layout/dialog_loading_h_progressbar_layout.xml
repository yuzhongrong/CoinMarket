<?xml version="1.0" encoding="utf-8"?>
<com.hjq.shape.layout.ShapeLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/dp110"
    android:layout_height="@dimen/dp110"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:shape_solidColor="@color/primaryBackDark"
    app:shape_radius="@dimen/dp15"
    android:orientation="vertical"


    >


    <com.zksg.kudoud.widgets.CircularProgressBar
        android:id="@+id/progress_bar_cus"
        android:layout_width="@dimen/dp55"
        android:layout_height="@dimen/dp55"
        android:layout_marginTop="@dimen/dp30"
        app:maxProgress="100"
        app:progress="0"
        app:progressbarBackgroundColor="@color/colorTextNormal"
        app:progressbarColor="@color/colorAccent"
        app:radius="@dimen/dp80"
        app:strokeWidth="@dimen/dp4"
        app:text="0%"
        app:p_textColor="@color/white"
        app:p_textSize="@dimen/sp13"
        android:layout_gravity="center"

        />




</com.hjq.shape.layout.ShapeLinearLayout>
